    "paths": {
        "/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get all users",
                "responses": {
                    "200": {
                        "description": "Success to get all users"
                    }
                }
            }
        },
        "/users/{userId}": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get user detail",
                "responses": {
                    "200": {
                        "description": "Success to get user detail"
                    }
                }
            }
        },
        "/users/signup": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Signup user",
                "responses": {
                    "200": {
                        "description": "Success to signup user"
                    }
                }
            }
        },
        "/users/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login user",
                "responses": {
                    "200": {
                        "description": "Success to login user"
                    }
                }
            }
        },
        "/users/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout user",
                "responses": {
                    "200": {
                        "description": "Success to logout user"
